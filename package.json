{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "BoilerPlate web app built using React and Redux",
  "main": "server.js",
  "scripts": {
    "build": "webpack --env=dev --progress --profile --colors",
    "dev": "webpack-dev-server --progress --colors --port 9000 --inline --content-base",
    "start": "webpack-dev-server --progress --colors --port 9000 --inline --content-base",
    "serve": "webpack-dev-server --progress --colors --port 9000 --inline --content-base",
    "build:dist": "webpack --env=prod --progress --profile --colors && cp index.html dist/ && cp server.js dist/ && cp -r images dist/",
    "serve:dist": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint src",
    "flow": "flow"
  },
  "author": "Kazim Ahmed Mohammed",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "express": "^4.14.1",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-meta-tags": "^0.1.3",
    "react-popover": "^0.4.8",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.45.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
